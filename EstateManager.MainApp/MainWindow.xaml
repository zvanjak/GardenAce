<Window x:Class="EstateManager.MainApp.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:EstateManager.MainApp"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800" MouseLeftButtonDown="Window_MouseLeftButtonDown" MouseLeftButtonUp="Window_MouseLeftButtonUp" MouseMove="Window_MouseMove" MouseRightButtonDown="Window_MouseRightButtonDown" MouseRightButtonUp="Window_MouseRightButtonUp" MouseWheel="Window_MouseWheel">
  <DockPanel>
    <Viewbox MouseLeftButtonDown="Viewbox_MouseLeftButtonDown">
      <!--<Canvas Width="321" Height="201">-->
      <!-- The Viewport3D provides a rendering surface for 3-D visual content. -->
      <Viewport3D Name="myViewport3D" ClipToBounds="True" Width="800" Height="500" Canvas.Left="0" Canvas.Top="10">
      </Viewport3D>
      <!--</Canvas>-->
    </Viewbox>
  </DockPanel>
</Window>
